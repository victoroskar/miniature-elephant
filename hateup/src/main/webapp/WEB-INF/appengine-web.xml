<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>natural-client-801</application>
    <version>1</version>
    <threadsafe>true</threadsafe>
	<use-google-connector-j>true</use-google-connector-j>
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
        <property name="cloudsql.url.dev" value="jdbc:mysql://localhost:3306/hateup"/>
        <property name="cloudsql.url.dev.user" value="root"/>
		<property name="cloudsql.url.prod" value="jdbc:google:mysql://natural-client-801:hateup-primary"/>
		<property name="cloudsql.url.prod.user" value="root"/>
    </system-properties>
</appengine-web-app>
